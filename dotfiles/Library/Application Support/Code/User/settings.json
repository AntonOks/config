{
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "bounded",
  "editor.scrollBeyondLastLine": false,
  "editor.scrollBeyondLastColumn": 0,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.renderIndentGuides": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.linkedEditing": true,
  "editor.codeActionsOnSave": ["source.fixAll", "source.organizeImports"],
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "always",
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.snippetSuggestions": "top",
  "editor.definitionLinkOpensInPeek": true,
  "editor.fontSize": 14,
  "editor.fontFamily": "Monaco, Fira Code",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "terminal.integrated.minimumContrastRatio": 4.5,
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontWeight": "100",
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.rendererType": "auto",
  "terminal.integrated.experimentalRefreshOnResume": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.experimentalLinkProvider": true,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "timeline.showView": false,
  "breadcrumbs.enabled": false,
  "extensions.ignoreRecommendations": false,
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "code-runner.enableAppInsights": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "workbench.quickOpen.preserveInput": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.enableExperiments": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": false,
  "workbench.editor.limit.value": 16,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.enablePreview": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Monokai",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.codeLens": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.nativeTabs": true,
  "window.titleBarStyle": "custom",
  "window.restoreFullscreen": false,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "typescript.format.enable": false,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.tsserver.useSeparateSyntaxServer": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.surveys.enabled": false,
  "typescript.suggest.paths": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.enable": false,
  "python.jediEnabled": true,
  "python.linting.pylamaEnabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.watcherExclude": {
    "**/.cache/**": true,
    "**/.webpack/**": true,
    "**/.idea/**": true,
    "**/node_modules/**": true
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/bower_components": true,
    "**/bower_packages": true
  },
  "files.autoSave": "onWindowChange",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": false,
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "typescript",
    "*.css": "postcss"
  },
  "files.saveConflictResolution": "askUser",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "postcss": "css",
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showSuggestionsAsSnippets": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": false,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "scm.providerCountBadge": "auto",
  "search.exclude": {
    "**/.cache": true,
    "**/.out": true,
    "**/node_modules": true,
    "**/bower_packages": true,
    "**/build": true,
    "**/dist": true
  },
  "search.searchOnType": false,
  "search.collapseResults": "alwaysCollapse",
  "task.autoDetect": "on",
  "task.quickOpen.detail": false,
  "npm.autoDetect": "on",
  "go.gopath": "~/go",
  "go.toolsGopath": "~/go/bin",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.languageServerFlags": ["serve", "-rpc.trace", "--debug=localhost:6060"],
  "reason_language_server.location": "/usr/local/bin/reason-language-server",
  "rust-analyzer.updates.askBeforeDownload": false,
  "rust-analyzer.lens.methodReferences": true,
  "docker.images.label": "Tag",
  "python.languageServer": "Pylance",
  "python.analysis.completeFunctionParens": true,
  "python.analysis.typeCheckingMode": "strict",
  "cSpell.enabled": false,
  "zenMode.restore": false
}
